{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "exclude": [
        "**/*.d.ts",
        "src/array/**/*",
        "src/heap/**/*",
        "src/segmentTree/**/*",
        "src/trie/**/*"
    ],
    "extension": [".ts", ".tsx"],
    "include": [
        "src/**/*"
    ],
    "reporter": [
        "text", "html"
    ],
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90
}